meta {
  name: Login Admin
  type: http
  seq: 1
}

post {
  url: {{host}}/auth/login
  body: json
  auth: none
}

body:json {
  {
    "username": "{{username}}",
    "password": "{{password}}"
  }
}

script:post-response {
  if (res.status === 200) {
    bru.setVar("authToken", res.body.token); 
    console.log("Token capturado correctamente");
  }
}
