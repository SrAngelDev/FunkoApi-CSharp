meta {
  name: Crear Funko (Admin)
  type: http
  seq: 2
}

post {
  url: {{host}}/funkos
  body: json
  auth: bearer
}

auth:bearer {
  token: {{authToken}}
}

body:json {
  {
    "nombre": "Funko Test Bruno",
    "precio": 19.99,
    "categoriaId": "b3e5e6a0-5678-4abc-8def-1234567890ab" 
  }
}

assert {
  res.status: eq 201
  res.body.nombre: eq "Funko Test Bruno"
  res.body.id: isNumber
}
